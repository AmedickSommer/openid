{
	"name": "friendsoftypo3/openid",
	"type": "typo3-cms-extension",
	"description": "TYPO3 Core",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0+"],

	"require": {
		"typo3/cms-core": "*",
		"typo3/cms-sv": "*",
		"typo3/cms-setup": "*"
	},
	"replace": {
		"openid": "self.version",
		"typo3/cms-openid": "self.version"
	},
	"autoload": {
		"psr-4": {
			"FoT3\\Openid\\": "Classes/"
		}
	},
	"extra": {
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"Migrations/Code/ClassAliasMap.php"
			]
		}
	}
}
