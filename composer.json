{
	"name": "friendsoftypo3/openid",
	"type": "typo3-cms-extension",
	"keywords": ["typo3","openid"],
	"description": "OpenID authentication for TYPO3 CMS",
	"license": ["GPL-2.0+"],
	"homepage": "https://github.com/FriendsOfTYPO3/openid",
	"authors": [
		{
			"name": "Dmitry Dulepov",
			"email": "dmitry@typo3.org",
			"role": "Developer"
		},
		{
			"name": "Markus Klein",
			"email": "markus.klein@typo3.org",
			"role": "Maintainer"
		}
	],
	"require": {
		"typo3/cms-core": "~7.6",
		"typo3/cms-sv": "~7.6",
		"typo3/cms-setup": "~7.6"
	},
	"replace": {
		"openid": "self.version",
		"typo3/cms-openid": "self.version",
		"typo3-ter/openid": "self.version"
	},
	"autoload": {
		"psr-4": {
			"FoT3\\Openid\\": "Classes/"
		}
	},
	"extra": {
		"typo3/class-alias-loader": {
			"class-alias-maps": [
				"Migrations/Code/ClassAliasMap.php"
			]
		}
	}
}
